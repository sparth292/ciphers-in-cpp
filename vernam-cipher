// Online C++ compiler to run C++ program online
#include <iostream>
// output should be COH
int main() {
  
    std::string str = "OAKSLAB";
    std::string key = "SON";
    std::string finalKey = "";
    int j = 0;
    
    
    for( int i = 0 ; i < str.length() ; i++ ){
        
        if(j==key.length()) j = 0;
        
        finalKey += key[j];
        
        j++;
        
    }
    
    int cipherCode;
    
    for( int i = 0 ; i < str.length() ; i++ ){
        
        cipherCode = (int)((str[i] - 'A') ^ (finalKey[i] - 'A'));
        
        if(cipherCode >= 26) 
        std::cout << (char)(cipherCode + 65 - 26) << std::endl;
        else
        std::cout << (char)(cipherCode + 65) << std::endl;
        
        
    }
    
    return 0;
}
